char* _strcat(char* dest, const char* src) {
	    int dest_len = 0;
	        int i = 0;
		    
		         while (dest[i] != '\0') {
		                dest_len++;
		                         i++;
		                             }
		    
		                                     i = 0;
		                                         while (src[i] != '\0') {
		                                                 dest[dest_len] = src[i];
		                                                         dest_len++;
		                                                                 i++;
		    
		    
